<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>


  <filter>
    <filter-name>FilterByXML</filter-name>
    <filter-class>com.pacemrc.javaweb.filter.FilterByXML</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>FilterByXML</filter-name>
    <url-pattern>/test/xml/filter</url-pattern>
  </filter-mapping>


  <listener>
    <listener-class>com.pacemrc.javaweb.listener.ListenerByXML</listener-class>
  </listener>

  <servlet>
    <servlet-name>ServletByXML</servlet-name>
    <servlet-class>com.pacemrc.javaweb.servlet.ServletByXML</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletByXML</servlet-name>
    <url-pattern>/test/xml/servlet</url-pattern>
  </servlet-mapping>

</web-app>
